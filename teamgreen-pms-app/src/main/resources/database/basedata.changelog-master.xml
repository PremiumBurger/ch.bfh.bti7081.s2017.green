<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


    <changeSet id="basedata" author="vonac1">
        <!-- Gender Types -->
        <insert tableName="GenderType">
            <column name="id" value="1"/>
            <column name="description" value="Male"/>
        </insert>
        <insert tableName="GenderType">
            <column name="id" value="2"/>
            <column name="description" value="Female"/>
        </insert>
    </changeSet>

    <changeSet id="basedata_2" author="schms27">
        <!-- Appointment States -->
        <insert tableName="AppointmentStateType">
            <column name="id" value="1"/>
            <column name="description" value="NEW"/>
        </insert>
        <insert tableName="AppointmentStateType">
            <column name="id" value="2"/>
            <column name="description" value="POSTPONED"/>
        </insert>
        <insert tableName="AppointmentStateType">
            <column name="id" value="3"/>
            <column name="description" value="CANCELLED"/>
        </insert>
        <insert tableName="AppointmentStateType">
            <column name="id" value="4"/>
            <column name="description" value="CONFIRMED"/>
        </insert>
        <insert tableName="AppointmentStateType">
            <column name="id" value="5"/>
            <column name="description" value="FINISHED"/>
        </insert>
    </changeSet>
</databaseChangeLog>